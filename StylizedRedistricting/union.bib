@article{lee_hubona_2009,
Abstract = {Qualitative research is just as able as quantitative research to follow certain fundamental principles of logic in general and scientific reasoning in particular. Two such principles are the logic of modus ponens and the logic of modus tollens. In this essay, we frame different research approaches--positivist research, interpretive research, action research, and design research--in the forms of modus ponens and modus tollens. Three issues emerge from this framing and call into question how research is now conducted in the discipline of information systems. They are the issue of a common scientific basis, the issue of the fallacy of affirming the consequent, and the issue of summative validity. Both rigor and relevance in information systems research may be better achieved by attending to the three issues. [ABSTRACT FROM AUTHOR]},
Author = {Lee, Allen S. and Hubona, Geoffrey S.},
ISSN = {02767783},
Journal = {MIS Quarterly},
Keywords = {QUALITATIVE research, INFORMATION resources management, SCIENCE -- Philosophy, ESSAY (Literary form), POSITIVISM, LOGIC diagrams, LOGIC, Symbolic & mathematical, EXPERIMENTAL design, action research, design research, epistemology, interpretive research, methodology, positivist research, qualitative research, quantitative research, relevance, research methods, rigor, theory building, Theory testing},
Number = {2},
Pages = {237--262},
Title = {A SCIENTIFIC BASIS FOR RIGOR IN INFORMATION SYSTEMS RESEARCH.},
Volume = {33},
URL = {https://proxy.library.upenn.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=bth&AN=38225118&site=ehost-live},
Year = {2009},
}


