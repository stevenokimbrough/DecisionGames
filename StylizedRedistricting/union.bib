@article{crossley_laananen_1997,
author = {Crossley, William A. and Laananen, David H.},
title = {The Genetic Algorithm as an Automated Methodology for Helicopter Conceptual Design},
journal = {Journal of Engineering Design},
volume = {8},
number = {3},
pages = {231-250},
year = {1997},
doi = {10.1080/09544829708907963},

URL = {http://www.tandfonline.com/doi/abs/10.1080/09544829708907963},
eprint = {http://www.tandfonline.com/doi/pdf/10.1080/09544829708907963},
abstract = { Abstract The genetic algorithm (GA) is a search algorithm that mimics the patterns of natural selection and reproduction displayed by biological populations. The GA's operation as a non-calculus-based search and optimization method makes it a good candidate for use in conceptual design. This paper presents a discussion of the use of a GA as an automated approach to conceptual design of helicopters. To accomplish this task, a GA was combined with an industry-standard helicopter sizing code. This resulting genetic algorithm-based design code was then used to generate conceptual designs for the medium lift replacement helicopter and a hypothetical attack helicopter. Results of these design efforts are discussed, providing insight into the use of a GA to automate the conceptual design of helicopters. }
}


