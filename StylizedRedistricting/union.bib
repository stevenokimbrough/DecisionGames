@article{Junni09072013,
author = {Junni, Paulina and Sarala, Riikka and Taras, Vas and Tarba, Shlomo}, 
title = {Organizational Ambidexterity and Performance: A Meta-Analysis},
year = {2013}, 
doi = {10.5465/amp.2012.0015}, 
abstract ={Organizational ambidexterity (OA) is argued to be increasingly important for the sustained competitive advantage of firms; this is reflected in a growing number of studies on the topic. However, prior OA studies have been conducted in a wide variety of industries and methodological settings and the empirical results have been mixed. Thus, the purpose of this article is to systematically examine the OA-performance relationship in order to reconcile the mixed results of prior research. By conducting a meta-analysis of prior studies on OA and performance, we find that positive and significant OA-performance relationships are to a large extent moderated by contextual factors and methodological choices: OA is particularly important for performance in non-manufacturing industries and at higher levels of analysis. Also, the performance effects are stronger when 'combined' measures of OA and perceptual performance measures are used and when a cross-sectional or multi-method research design is applied. Our meta-analysis findings advance the field of OA by revealing some of the moderators in the OA-performance relationship, thereby providing a more fine-grained understanding of the effects of OA on firm performance in prior studies. Also, the findings allow us to point to any gaps and methodological weaknesses in prior OA research and consequently to propose avenues for future research in the OA field. }, 
URL = {http://amp.aom.org/content/early/2013/07/09/amp.2012.0015.abstract}, 
eprint = {http://amp.aom.org/content/early/2013/07/09/amp.2012.0015.full.pdf+html}, 
journal = {The Academy of Management Perspectives}, 
volume={27},
number={4},
pages={299--312}
}

