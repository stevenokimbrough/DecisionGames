@article{hsu_huang_2005,
Abstract = {The user fatigue problem in interactive evolutionary computation (IEC) is a complex and interesting issue. If the IEC search space is created from the experience or knowledge of domain experts rather than from users values, it causes two potential problems which lead to fatigue problems in IEC: 1) inefficiency and 2) boredom. Therefore, we propose a customer values-based IEC model, solving the fatigue problem by avoiding the potential problems. A case study involving the design of mineral water bottles was used to verify the anti-fatigue capability of the users when using the proposed model For comparison with the traditional domain knowledge-based model, we built two IEC systems, a customer values-based system and a traditional system, and conducted a user burden test and a system efficiency test over a two-week period. The results of both tests show that our proposed system performed better than the traditional system in designing mineral water bottles. [ABSTRACT FROM AUTHOR]},
Author = {Fang-Cheng Hsu and Peter Huang},
ISSN = {02883635},
Journal = {New Generation Computing},
Keywords = {EVOLUTIONARY computation, NEURAL networks (Computer science), MINERAL water bottles, EXPERT systems (Computer science), MATHEMATICAL models, Customer Design, Interactive Evolutionary Computation, User Fatigue, Value-Focused Thinking},
Number = {2},
Pages = {115 - 127},
Title = {Providing an Appropriate Search Space to Solve the Fatigue Problem in Interactive Evolutionary Computation.},
Volume = {23},
URL = {https://proxy.library.upenn.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=keh&AN=17188691&site=ehost-live},
Year = {2005},
}


